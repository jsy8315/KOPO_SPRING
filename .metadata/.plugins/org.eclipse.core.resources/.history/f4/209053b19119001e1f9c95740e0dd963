package com.openbanking.dto;

import java.sql.Timestamp;

public class BoardDto {

	int bId;
	//글의 id, 시퀀스에 의해 자동으로 매겨짐
	String bName;
	String bTitle;
	String bContent;
	Timestamp bDate;
	int bHit;
	int bGroup;
	
	public BoardDto() {
		
	}
	
	public BoardDto(int bId, String bName, String bTitle, String bContent, Timestamp bDate, int bHit, int bGroup) {
		this.bId = bId;
		this.bName = bName;
		this.bTitle = bTitle;
		this.bContent = bContent;
		this.bDate = bDate;
		this.bHit = bHit;
		this.bGroup = bGroup;	
	}
}

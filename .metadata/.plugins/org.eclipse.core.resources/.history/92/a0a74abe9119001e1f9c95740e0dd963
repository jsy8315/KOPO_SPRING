package com.openbanking.dto;

import java.sql.Timestamp;

public class BoardDto {

	int bId;
	//글의 id, 시퀀스에 의해 자동으로 매겨짐
	String bName; //작성자 이름
	String bTitle; //글 제목
	String bContent; //글 내용
	Timestamp bDate; //시간, 자동으로 현재 시간 세팅되게
	int bHit; //조회수
	int bGroup; //
	
	public BoardDto() {
		
	}
	
	public BoardDto(int bId, String bName, String bTitle, String bContent, Timestamp bDate, int bHit, int bGroup) {
		this.bId = bId;
		this.bName = bName;
		this.bTitle = bTitle;
		this.bContent = bContent;
		this.bDate = bDate;
		this.bHit = bHit;
		this.bGroup = bGroup;	
	}
}
